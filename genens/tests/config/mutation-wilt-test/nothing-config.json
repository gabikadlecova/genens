{
   "datasets": [
        { "dataset_name": "wilt", "split_validation": false }
    ],
   "parameters": {
      "n_jobs": -1,
      "pop_size": 200,
      "n_gen": 15,
      "cx_pb": 0.0,
      "mut_pb": 0.0,
      "mut_args_pb": 0.0,
      "mut_node_pb": 0.0,
      "max_height": 5,
      "timeout": 420
   },
   "scorer": {
      "func": "sklearn.metrics.cohen_kappa_score",
      "kwargs": {
          "weights": "quadratic"
      }
   },
   "evaluator": {
          "func": "sample_crossval",
          "kwargs": {
	       "cv_k": 5,
	       "sample_size": 0.25,
	       "per_gen": true
          }
   },
   "group_weights":  {
            "pipeline": 1.0,
            "union": 0.3,
            "prepro": 1.0,
            "ensemble": 0.5,
            "ensemble_l": 1.0,
            "predictor": 1.0,
            "transform": 1.0
   }
}
