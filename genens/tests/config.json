{
   "datasets": [
        { "dataset_name": "wilt", "split_validation": false }
    ],
   "parameters": {
      "n_jobs": 1,
      "pop_size": 4,
      "n_gen": 15,
      "cx_pb": 0.5,
      "mut_pb": 0.3,
      "mut_args_pb": 0.6,
      "max_height": [4, 7, 9],
      "timeout": 420
   },
   "scorer": {
          "func": "sklearn.metrics.cohen_kappa_score",
          "kwargs": {
              "weights": "quadratic"
          }
   },
   "evaluator": {
          "func": "crossval",
          "kwargs": {
                "cv_k": 3
          }
   },
   "group_weights":  {
            "pipeline": 1.0,
            "union": [0.3, 0.5, 0.7],
            "prepro": 1.0,
            "ensemble": [0.5, 0.7],
            "ensemble_l": [1.0, 1.2],
            "predictor": 1.0,
            "transform": 1.0
   }
}
