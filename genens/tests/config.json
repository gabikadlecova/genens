{
   "datasets": [
        { "dataset_name": "mnist", "split_validation": false }
    ],
   "parameters": {
      "n_jobs": 1,
      "pop_size": 4,
      "n_gen": 15,
      "cx_pb": 0.5,
      "mut_pb": 0.3,
      "mut_args_pb": 0.6,
      "max_height": 4,
      "timeout": 420
   },
   "scorer": {
          "func": "sklearn.metrics.cohen_kappa_score",
          "kwargs": {
              "weights": "quadratic"
          }
   },
   "evaluator": {
          "func": "sample_crossval",
          "kwargs": {
               "cv_k": 5,
               "per_gen": true,
               "sample_size": 0.01,
               "random_state": 42
          }
   },
   "group_weights":  {
            "pipeline": 1.0,
            "union": 0.3,
            "prepro": 1.0,
            "ensemble": 0.1,
            "ensemble_l": 1.2,
            "predictor": 1.0,
            "transform": 1.0
   }
}
