{
   "datasets": [
        { "dataset_name": "wilt", "split_validation": false }
    ],
   "parameters": {
      "n_jobs": -1,
      "pop_size": 200,
      "n_gen": 15,
      "cx_pb": 0.5,
      "mut_pb": 0.3,
      "mut_args_pb": 0.6,
      "mut_node_pb": 0.3,
      "max_height": [4, 8],
      "timeout": 420
   },
   "scorer": {
          "func": "sklearn.metrics.cohen_kappa_score",
          "kwargs": {
              "weights": "quadratic"
          }
   },
   "evaluator": {
          "func": "sample_crossval",
          "kwargs": {
               "per_gen": [true, false],
               "random_state": [41, 42, 43],
	       "cv_k": 5,
               "sample_size": 0.25
          }
   },
   "group_weights":  {
            "pipeline": 1.0,
            "union": 0.3,
            "prepro": 1.0,
            "ensemble": 0.5,
            "ensemble_l": 1.0,
            "predictor": 1.0,
            "transform": 1.0
   }
}
